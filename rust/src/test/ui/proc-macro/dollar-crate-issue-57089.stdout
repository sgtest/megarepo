PRINT-BANG INPUT (DISPLAY): struct M($crate :: S) ;
PRINT-BANG INPUT (DEBUG): TokenStream [
    Ident {
        ident: "struct",
        span: #CTXT bytes(LO..HI),
    },
    Ident {
        ident: "M",
        span: #CTXT bytes(LO..HI),
    },
    Group {
        delimiter: Parenthesis,
        stream: TokenStream [
            Ident {
                ident: "$crate",
                span: #CTXT bytes(LO..HI),
            },
            Punct {
                ch: ':',
                spacing: Joint,
                span: #CTXT bytes(LO..HI),
            },
            Punct {
                ch: ':',
                spacing: Alone,
                span: #CTXT bytes(LO..HI),
            },
            Ident {
                ident: "S",
                span: #CTXT bytes(LO..HI),
            },
        ],
        span: #CTXT bytes(LO..HI),
    },
    Punct {
        ch: ';',
        spacing: Alone,
        span: #CTXT bytes(LO..HI),
    },
]
