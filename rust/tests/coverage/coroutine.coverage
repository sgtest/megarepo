   LL|      1|fn get_u32(val: bool) -> Result<u32, String> {
   LL|      1|    if val { Ok(1) } else { Err(String::from("some error")) }
                                          ^0
   LL|      1|}
   LL|       |
   LL|      1|fn main() {
   LL|      1|    let is_true = std::env::args().len() == 1;
   LL|      1|    let mut coroutine = || {
