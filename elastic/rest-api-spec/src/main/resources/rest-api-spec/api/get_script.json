{
  "get_script": {
    "documentation": "http://www.elastic.co/guide/en/elasticsearch/reference/master/modules-scripting.html",
    "stability": "stable",
    "methods": ["GET"],
    "url": {
      "paths": [ "/_scripts/{id}" ],
      "parts": {
        "id": {
          "type" : "string",
