          "trigger": {
            "type": "object",
            "enabled": false,
            "dynamic": true
          },
          "input": {
            "type": "object",
            "enabled": false,
            "dynamic": true
          },
          "condition": {
            "type": "object",
            "enabled": false,
            "dynamic": true
          },
          "throttle_period": {
            "type": "keyword",
            "index": false,
            "doc_values": false
          },
          "throttle_period_in_millis": {
            "type": "long",
            "index": false,
            "doc_values": false
          },
          "transform": {
            "type": "object",
            "enabled": false,
            "dynamic": true
          },
          "actions": {
            "type": "object",
            "enabled": false,
            "dynamic": true
          },
          "metadata": {
            "type": "object",
            "dynamic": true
