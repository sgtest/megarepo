{
  "index_patterns": [
    ".fleet-file-data-*-*"
  ],
  "priority": 200,
  "composed_of": [],
  "_meta": {
    "description": "fleet file data index template",
    "managed": true
  },
  "template" : {
    "settings": {
      "index.lifecycle.name": ".fleet-file-data-ilm-policy",
      "auto_expand_replicas": "0-1"
    },
    "mappings": {
      "_doc": {
        "_meta": {
          "version": "${xpack.fleet.template.version}"
        },
        "properties": {
          "data": {
            "type": "binary"
          },
          "bid": {
            "type": "keyword"
          },
          "sha2": {
            "type": "keyword",
            "index": false
          },
          "last": {
            "type": "boolean",
            "index": false
          }
        }
      }
    }
  },
  "version": ${xpack.fleet.template.version}
}