{
  "_meta":{
    "_rollup": {
      "ROLLUP_METADATA_PLACEHOLDER":"ROLLUP_METADATA_PLACEHOLDER"
    },
    "rollup-version": "${rollup.dynamic_template.version}"
  },
  "dynamic_templates": [
    {
      "strings": {
        "match_mapping_type": "string",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "date_histograms": {
        "path_match": "*.date_histogram.timestamp",
        "mapping": {
          "type": "date"
        }
      }
    }
  ]
}