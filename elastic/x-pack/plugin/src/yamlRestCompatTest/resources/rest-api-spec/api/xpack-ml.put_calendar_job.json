{
  "xpack-ml.put_calendar_job":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/ml-put-calendar-job.html",
      "description":"Adds an anomaly detection job to a calendar."
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/vnd.elasticsearch+json;compatible-with=7"]
    },
    "url":{
      "paths":[
        {
          "path":"/_xpack/ml/calendars/{calendar_id}/jobs/{job_id}",
          "methods":[
            "PUT"
          ],
          "parts":{
            "calendar_id":{
              "type":"string",
              "description":"The ID of the calendar to modify"
            },
            "job_id":{
              "type":"string",
              "description":"The ID of the job to add to the calendar"
            }
          },
          "deprecated":{
            "version":"7.0.0",
            "description":"all _xpack prefix have been deprecated"
          }
        }
      ]
    }
  }
}
