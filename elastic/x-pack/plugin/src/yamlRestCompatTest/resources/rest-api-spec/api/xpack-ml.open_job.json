{
  "xpack-ml.open_job":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/ml-open-job.html",
      "description":"Opens one or more anomaly detection jobs."
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/vnd.elasticsearch+json;compatible-with=7"]
    },
    "url":{
      "paths":[
        {
          "path":"/_xpack/ml/anomaly_detectors/{job_id}/_open",
          "methods":[
            "POST"
          ],
          "parts":{
            "job_id":{
              "type":"string",
              "description":"The ID of the job to open"
            }
          },
          "deprecated":{
            "version":"7.0.0",
            "description":"all _xpack prefix have been deprecated"
          }
        }
      ]
    }
  }
}
