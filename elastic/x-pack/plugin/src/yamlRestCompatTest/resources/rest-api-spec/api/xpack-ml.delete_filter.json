{
  "xpack-ml.delete_filter":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/ml-delete-filter.html",
      "description":"Deletes a filter."
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/vnd.elasticsearch+json;compatible-with=7"]
    },
    "url":{
      "paths":[
        {
          "path":"/_xpack/ml/filters/{filter_id}",
          "methods":[
            "DELETE"
          ],
          "parts":{
            "filter_id":{
              "type":"string",
              "description":"The ID of the filter to delete"
            }
          },
          "deprecated":{
            "version":"7.0.0",
            "description":"all _xpack prefix have been deprecated"
          }
        }
      ]
    }
  }
}
