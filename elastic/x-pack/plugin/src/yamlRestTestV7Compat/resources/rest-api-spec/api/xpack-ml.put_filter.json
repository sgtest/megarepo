{
  "xpack-ml.put_filter":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/ml-put-filter.html",
      "description":"Instantiates a filter."
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/vnd.elasticsearch+json;compatible-with=7"],
      "content_type": ["application/json"]
    },
    "url":{
      "paths":[
        {
          "path":"/_xpack/ml/filters/{filter_id}",
          "methods":[
            "PUT"
          ],
          "parts":{
            "filter_id":{
              "type":"string",
              "description":"The ID of the filter to create"
            }
          },
          "deprecated":{
            "version":"7.0.0",
            "description":"all _xpack prefix have been deprecated"
          }
        }
      ]
    },
    "body":{
      "description":"The filter details",
      "required":true
    }
  }
}
