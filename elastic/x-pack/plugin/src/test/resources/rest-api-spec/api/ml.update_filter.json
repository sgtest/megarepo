{
  "ml.update_filter": {
    "stability": "stable",
    "methods": [ "POST" ],
    "url": {
      "paths": [ "/_ml/filters/{filter_id}/_update" ],
      "parts": {
        "filter_id": {
          "type": "string",
          "required": true,
          "description": "The ID of the filter to update"
        }
      }
    },
    "body": {
      "description" : "The filter update",
      "required" : true
    }
  }
}
