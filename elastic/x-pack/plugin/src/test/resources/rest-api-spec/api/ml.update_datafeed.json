{
  "ml.update_datafeed": {
    "documentation": "http://www.elastic.co/guide/en/elasticsearch/reference/current/ml-update-datafeed.html",
    "stability": "stable",
    "methods": [ "POST" ],
    "url": {
      "paths": [ "/_ml/datafeeds/{datafeed_id}/_update" ],
      "parts": {
        "datafeed_id": {
          "type": "string",
          "required": true,
          "description": "The ID of the datafeed to update"
        }
      }
    },
    "body": {
      "description" : "The datafeed update settings",
      "required" : true
    }
  }
}
