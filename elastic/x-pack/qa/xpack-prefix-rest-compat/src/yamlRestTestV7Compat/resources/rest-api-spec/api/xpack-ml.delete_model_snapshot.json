{
  "xpack-ml.delete_model_snapshot":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/ml-delete-snapshot.html",
      "description":"Deletes an existing model snapshot."
    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/vnd.elasticsearch+json;compatible-with=7"]
    },
    "url":{
      "paths":[
        {
          "path":"/_xpack/ml/anomaly_detectors/{job_id}/model_snapshots/{snapshot_id}",
          "methods":[
            "DELETE"
          ],
          "parts":{
            "job_id":{
              "type":"string",
              "description":"The ID of the job to fetch"
            },
            "snapshot_id":{
              "type":"string",
              "description":"The ID of the snapshot to delete"
            }
          },
          "deprecated":{
            "version":"7.0.0",
            "description":"all _xpack prefix have been deprecated"
          }
        }
      ]
    }
  }
}
