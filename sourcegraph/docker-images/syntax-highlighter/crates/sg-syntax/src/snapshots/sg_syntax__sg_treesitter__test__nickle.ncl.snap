        []
      else
//    ^^^^ Keyword
        array.generate (fun x => x + start) (end - start)
//      ^^^^^^^^^^^^^^ IdentifierFunction
//                      ^^^ Keyword
  in
  
  let is_prime
    | doc "Returns true if the argument is a prime number."
//         ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^ StringLiteral
    = fun x => x > 1 && array.all (fun d => x % d != 0) (range 2 (x - 1))
//    ^^^ Keyword
