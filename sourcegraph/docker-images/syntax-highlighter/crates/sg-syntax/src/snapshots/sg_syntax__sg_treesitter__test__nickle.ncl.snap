  let primes
    | doc "Generate `max` primes using Sieve of Eratosthenes."
//         ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^ StringLiteral
    | Number -> Array Prime
//    ^^^^^^^^^^ IdentifierType
//              ^^^^^ IdentifierBuiltinType
//                   ^^^^^^ IdentifierType
    = fun max =>
//    ^^^ Keyword
      let limit = number.pow max (1 / 2) in # sqrt(max)
//                ^^^^^^^^^^ IdentifierFunction
//                           ^^^ IdentifierFunction
//                                ^ NumericLiteral
//                                    ^ NumericLiteral
//                                          ^^^^^^^^^^^ Comment
      let drop_multiples = fun x xs =>
//                         ^^^ Keyword
        let to_drop = max
          |> array.generate (fun y => (y + 2) * x)
//           ^^^^^^^^^^^^^^ IdentifierFunction
//                           ^^^ Keyword
//                                         ^ NumericLiteral
//                                            ^ PunctuationDelimiter
          |> array.filter (fun y => y <= max) in
//           ^^^^^^^^^^^^ IdentifierFunction
//                         ^^^ Keyword
          |> array.flatten
//           ^^^^^^^^^^^^^ IdentifierFunction
        array.filter (fun y => array.all ((!=) y) to_drop) xs in
//      ^^^^^^^^^^^^ IdentifierFunction
//                    ^^^ Keyword
//                             ^^^^^^^^^ IdentifierFunction
//                                         ^^ IdentifierOperator
      let rec loop = fun x xs =>
//        ^^^ Keyword
//                   ^^^ Keyword
        if x > limit then
//      ^^ Keyword
//                   ^^^^ Keyword
          xs
        else
//      ^^^^ Keyword
          loop (x + 1) (drop_multiples x xs) in
//        ^^^^ IdentifierFunction
//                  ^ NumericLiteral
//                      ^^^^^^^^^^^^^^ IdentifierFunction
//                                     ^ IdentifierFunction
      loop 2 (range 2 max) in
//    ^^^^ IdentifierFunction
//         ^ NumericLiteral
//            ^^^^^ IdentifierFunction
//                  ^ NumericLiteral
  
