  let range
    | doc "Generate an array of integers in the range [`start`, `end`)."
//         ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^ StringLiteral
    | Number -> Number -> Array Number
//    ^^^^^^^^^^^^^^^^^^^^ IdentifierType
//                        ^^^^^ IdentifierBuiltinType
//                             ^^^^^^^ IdentifierType
    = fun start end =>
//    ^^^ Keyword
      if end <= start then
//    ^^ Keyword
