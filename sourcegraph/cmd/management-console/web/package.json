{
  "name": "management-console",
  "private": true,
  "dependencies": {
    "react": "^16.9.0",
    "rxjs": "^6.5.3"
  },
  "scripts": {
    "serve": "cp ../../../ui/assets/img/favicon.png ./src && ./node_modules/.bin/tsc --noEmit && ./node_modules/.bin/parcel serve --out-dir=../assets --no-source-maps -p 2680 src/index.html",
